<addons>
<addon id="plugin.video.xstream.reloaded"
	name="xStream"
	version="1.4.0"
	provider-name="murphy and membrane">
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="de">xStream Reloaded</summary>
    <description lang="de">xStream ist ein Plugin, welches mit eigene Plugins erweitern werden kann. Hiermit kann man Filme, Serien, etc per Stream schauen</description>
    <summary lang="en">xStream Reloaded</summary>
    <description lang="en">Plugin to watch streams from various film, trailer, music and sport sites</description>
    <platform>all</platform>
  </extension>
</addon>

